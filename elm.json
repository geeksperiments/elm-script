{
    "type": "package",
    "name": "ianmackenzie/script-experiment",
    "version": "1.0.0",
    "summary": "Experimental command-line scripting in Elm",
    "repository": "https://github.com/ianmackenzie/script-experiment.git",
    "license": "MPL-2.0",
    "exposed-modules": [
        "Script",
        "Script.Directory",
        "Script.EnvironmentVariables",
        "Script.File",
        "Script.FileSystem",
        "Script.NetworkConnection",
        "Script.Permissions",
        "Script.Platform",
